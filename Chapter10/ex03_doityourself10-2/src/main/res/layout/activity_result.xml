    <RatingBar
        style="@style/Widget.AppCompat.RatingBar.Small"
        android:id="@+id/rbar9"
        android:numStars="5">

    </RatingBar>


</TableRow>
    <Button
        android:id="@+id/btnResult"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="투표 종료"
        android:textSize="20dp"/>



</TableLayout>